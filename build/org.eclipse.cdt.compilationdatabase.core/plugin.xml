<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.cdt.core.LanguageSettingsProvider">
      <provider
            class="org.eclipse.cdt.compilationdatabase.internal.core.CompilationDatabaseParser"
            id="org.eclipse.cdt.compilationdatabase.core.CompilationDatabaseParser"
            name="Compilation Database Parser"
            prefer-non-shared="true">
      </provider>
   </extension>
   <extension
            point="org.eclipse.cdt.ui.LanguageSettingsProviderAssociation">
      <class-association
            class="org.eclipse.cdt.compilationdatabase.internal.core.CompilationDatabaseParser"
            icon="icons/obj16/log_obj.gif"
            page="org.eclipse.cdt.compilationdatabase.internal.ui.CompilationDatabaseParserOptionPage"
            ui-clear-entries="true"
            ui-edit-entries="false">
      </class-association>
   </extension>
</plugin>

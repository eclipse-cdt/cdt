   <extension
         point="org.eclipse.cdt.ui.cPropertyTab">

<!-- exported from CORE -->           
     <tab
           class="org.eclipse.cdt.ui.newui.IncludeTab"
           name="%Includes"
           icon="icons/obj16/hfolder_obj.gif"
           weight="010"

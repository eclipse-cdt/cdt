   <extension
         point="org.eclipse.ui.views">
      <category
            name="%ViewCatagoryMake.name"
            id="org.eclipse.cdt.make.ui">
      </category>
      <view
            name="%Make.Target.label"
            icon="icons/view16/make_target.gif"

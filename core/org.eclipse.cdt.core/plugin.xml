      <processType
            name="AppendCreate"
            processRunner="org.eclipse.cdt.core.templateengine.process.processes.AppendCreate">
         <simple name="projectName"/>
         <complexArray name="files">
            <baseType>
               <simple name="source"/>
               <simple name="target"/>
               <simple name="replaceable"/>

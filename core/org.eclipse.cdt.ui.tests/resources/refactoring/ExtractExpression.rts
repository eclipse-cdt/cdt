//!Extract boolean comparison from if-condition.
//#org.eclipse.cdt.ui.tests.refactoring.extractfunction.ExtractFunctionRefactoringTest

//@.config
filename=test.cpp
methodname=check

//@test.h
class Test {
	void test();
};


//=
class Test {
	void test();
	bool check();
};


//@test.cpp
#include "test.h"

void Test::test() {
	if (/*$*/5 == 3 + 2/*$$*/) {
		//...
	}
}

//=
#include "test.h"

bool Test::check() {
	return 5 == 3 + 2;
}

void Test::test() {
	if (check()) {
		//...
	}
}


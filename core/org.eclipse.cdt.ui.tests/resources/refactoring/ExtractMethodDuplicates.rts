//!ExtractFunctionRefactoringTest with Return Value and a lot Ref Parameter and a method call
//#org.eclipse.cdt.ui.tests.refactoring.extractfunction.ExtractFunctionRefactoringTest
//@.config
replaceduplicates=true
returnvalue=true
//@A.h
#ifndef A_H_
#define A_H_

#include "B.h"

class A {
public:
	A();
	virtual ~A();
	int foo();

private:
	int help();
};

#endif /*A_H_*/

//=
#ifndef A_H_
#define A_H_

#include "B.h"

class A {
public:
	A();
	virtual ~A();
	int foo();

private:
	int help();
	int exp(int i, B*& b, int& y, float& x);
};

#endif /*A_H_*/

//@A.cpp
#include "A.h"

A::A() {
}

A::~A() {
	int i = 2;
	float x = i;
	B* b = new B();
	int y = x + i;
	++i;
	b->hello(y);
	i = i + x;
	help();
	b->hello(y);
	++x;
	i++;
}

int A::foo() {
	int i = 2;
	float x = i;
	B* b = new B();
	int y = x + i;
	/*$*/++i;
	b->hello(y);
	i = i + x;
	help();/*$$*/
	b->hello(y);
	++x;
	return i;
}

int A::help() {
	return 42;
}

//=
#include "A.h"

A::A() {
}

A::~A() {
	int i = 2;
	float x = i;
	B* b = new B();
	int y = x + i;
	i = exp(i, b, y, x);
	b->hello(y);
	++x;
	i++;
}

int A::exp(int i, B*& b, int& y, float& x) {
	++i;
	b->hello(y);
	i = i + x;
	help();
	return i;
}

int A::foo() {
	int i = 2;
	float x = i;
	B* b = new B();
	int y = x + i;
	i = exp(i, b, y, x);
	b->hello(y);
	++x;
	return i;
}

int A::help() {
	return 42;
}


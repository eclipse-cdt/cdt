//!ExtractFunctionRefactoringTest with duplicates
//#org.eclipse.cdt.ui.tests.refactoring.extractfunction.ExtractFunctionRefactoringTest
//@.config
replaceduplicates=true
//@A.h
#ifndef A_H_
#define A_H_

class A {
public:
	A();
	virtual ~A();
	int foo();

private:
	int help();
};

#endif /*A_H_*/

//=
#ifndef A_H_
#define A_H_

class A {
public:
	A();
	virtual ~A();
	int foo();

private:
	int help();
	void exp(int& i);
};

#endif /*A_H_*/

//@A.cpp
#include "A.h"

A::A() {
}

A::~A() {
	int i = 2;
	++i;
	help();
}

int A::foo() {
	int i = 2;
	/*$*/++i;
	help();/*$$*/
	return i;
}

int A::help() {
	return 42;
}

//=
#include "A.h"

A::A() {
}

A::~A() {
	int i = 2;
	exp(i);
}

void A::exp(int& i) {
	++i;
	help();
}

int A::foo() {
	int i = 2;
	exp(i);
	return i;
}

int A::help() {
	return 42;
}


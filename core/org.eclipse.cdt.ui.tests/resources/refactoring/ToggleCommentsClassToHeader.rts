//!ClassToHeaderTemplateTopComment
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
class A {
	// Top comment
	template<typename T>
	T /*$*/member/*$$*/() {
		return T();
	}
};
//=
class A {
	// Top comment
	template<typename T>
	T member();

};

// Top comment
template<typename T>
inline T A::member() {
	return T();
}


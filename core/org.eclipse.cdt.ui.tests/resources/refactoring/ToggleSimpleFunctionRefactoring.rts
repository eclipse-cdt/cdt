//!Test simple function in class to in header
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
#include <iostream>

class A {
public:
	int /*$*/function/*$$*/() {
		return 0;
	}

private:
	int a;
};
//=
#include <iostream>

class A {
public:
	int function();

private:
	int a;
};

inline int A::function() {
	return 0;
}

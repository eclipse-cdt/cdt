//!Test template function in class to in header
//#org.eclipse.cdt.ui.tests.refactoring.togglefunction.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
#include <iostream>

template <typename T, typename U>
class A {
	class B {
		T /*$*/member/*$$*/() {
			return T();
		}
	};
};
//=
#include <iostream>

template <typename T, typename U>
class A {
	class B {
		T member();
	};
};

template<typename T, typename U>
inline T A<T, U>::B::member() {
	return T();
}

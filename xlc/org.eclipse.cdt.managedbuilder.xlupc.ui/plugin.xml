                  sourceContentType="org.eclipse.cdt.core.cSource"
                  sources="c">
            </inputType>
            <outputType
                  buildVariable="OBJS"
                  id="org.eclipse.cdt.managedbuilder.xlupc.ui.outputType.objectFile"
                  outputs="o">
